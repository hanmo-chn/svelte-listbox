{
  "name": "@hanmotec/tsui-listbox",
  "version": "0.0.6",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run build:css && svelte-kit sync && svelte-package",
    "build:css": "sass src/lib/tsui-listbox.scss:src/lib/tsui-listbox.css --no-source-map",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "publish": "cd ./package && npm publish --access public"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "node-sass": "^8.0.0",
    "sass": "^1.57.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "4.0.4"
  },
  "author": "Henry Feng",
  "license": "MIT"
}
